<!-- Menu Toggle Script -->
<!--아래 스크립트 적용을 위해서는 id가 wrapper 인 div 안에 모두 감싸야 합니다. -->
<script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    $(document).ready(function(){
        $("#nav ul.sub").hide();
        $("#nav ul.menu li").click(function(){
            $("ul",this).slideToggle("toggled");
        });
    });
</script>
</body>
</html>